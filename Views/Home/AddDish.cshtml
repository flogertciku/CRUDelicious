@model Dish

<form asp-action="CreateDish" method="post">
    <div>
        <label asp-for="Name"></label>
        <input asp-for="Name">
        <span asp-validation-for="Name"></span>
    </div>
    <div>
        <label asp-for="Chef"></label>
        <input asp-for="Chef">
        <span asp-validation-for="Chef"></span>
    </div>
    <div>
        <label asp-for="Tastiness"></label>
        <select asp-for="Tastiness" id="">
            <option value="1">1</option>
            <option value="2">2</option>
            <option value="3">3</option>
            <option value="4">4</option>
            <option value="5">5</option>
        </select>
        <span asp-validation-for="Tastiness"></span>
    </div>
    <div>
        <label asp-for="Calories"></label>
        <input asp-for="Calories">
        <span asp-validation-for="Calories"></span>
    </div>
     <div>
        <label asp-for="Description"></label>
        <input asp-for="Description">
        <span asp-validation-for="Description"></span>
    </div>

    <input type="submit" value="Add Dish" name="" id="">


</form>